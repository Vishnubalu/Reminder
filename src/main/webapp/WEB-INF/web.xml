<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>getUserController</servlet-name>
  	<display-name>getUserController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.getUserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>signupController</servlet-name>
  	<display-name>signupController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.signupController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>noteBooksController</servlet-name>
  	<display-name>noteBooksController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.noteBooksController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>editUserController</servlet-name>
  	<display-name>editUserController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.editUserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>noteController</servlet-name>
  	<display-name>noteController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.noteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>dashboardController</servlet-name>
  	<display-name>dashboardController</display-name>
  	<description></description>
  	<servlet-class>com.vishnu.web.dashboardController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>getUserController</servlet-name><url-pattern>/getUser</url-pattern>
  	
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>signupController</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>noteBooksController</servlet-name>
  	<url-pattern>/notebooks</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>editUserController</servlet-name>
  	<url-pattern>/editUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>noteController</servlet-name>
  	<url-pattern>/notecontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>dashboardController</servlet-name>
  	<url-pattern>/dashboardController</url-pattern>
  </servlet-mapping>
</web-app>
